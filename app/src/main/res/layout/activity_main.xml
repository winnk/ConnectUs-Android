<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kaya.connectus2.MainActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/greenhaze"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/greenhaze"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"/>

    <ImageButton
        android:id="@+id/reminderButton"
        android:layout_width="68dp"
        android:layout_height="67dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="103dp"
        android:layout_marginStart="103dp"
        android:layout_marginTop="155dp"
        android:tint="@color/colorPrimaryDark"
        app:layout_constraintBottom_toTopOf="@+id/noteButton"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_lock_idle_alarm"/>

    <ImageButton
        android:id="@+id/symptomSearchButton"
        android:layout_width="wrap_content"
        android:layout_height="68dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="43dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="155dp"
        android:tint="?attr/colorPrimary"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/reminderButton"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_search_category_default"/>

    <ImageButton
        android:id="@+id/noteButton"
        android:layout_width="71dp"
        android:layout_height="66dp"
        android:layout_marginLeft="100dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="264dp"
        android:tint="?android:attr/colorPrimaryDark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_edit"/>

    <ImageButton
        android:id="@+id/calendarButton"
        android:layout_width="65dp"
        android:layout_height="64dp"
        android:layout_marginLeft="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="41dp"
        android:tint="@color/colorPrimaryDark"
        app:layout_constraintLeft_toRightOf="@+id/noteButton"
        app:layout_constraintTop_toBottomOf="@+id/symptomSearchButton"
        app:srcCompat="@android:drawable/ic_menu_my_calendar"/>

</android.support.constraint.ConstraintLayout>
